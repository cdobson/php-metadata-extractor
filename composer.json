{
	"name" : "gomoob/php-metadata-extractor",
	"description" : "PHP wrapper to easily call the Java metadata-extrator library.",
	"version" : "2.9.1-alpha.3",
	"license" : "MIT",
	"type" : "library",
	"keywords" : [
		"gomoob",
		"metadata-extractor",
		"image",
		"metadata",
		"photo",
		"exif"
	],
	"authors" : [{
			"name" : "GoMoob",
			"email" : "contact@gomoob.com",
			"homepage" : "http://www.gomoob.com"
		}, {
			"name" : "Baptiste Gaillard",
			"email" : "baptiste.gaillard@gomoob.com"
		}, {
			"name" : "Simon Baudry",
			"email" : "simon.baudry@gomoob.com"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Gomoob\\" : "src/main/php/Gomoob"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Gomoob\\" : "src/test/php/Gomoob"
		}
	},
	"require" : {
		"php" : "^5.6 || ^7.0",
		"alchemy/binary-driver" : "^1.6.0"
	},
	"require-dev" : {
		"codeclimate/php-test-reporter" : "^0.3.2",
		"pdepend/pdepend" : "^2.2.4",
		"phpdocumentor/phpdocumentor" : "^2.9.0",
		"phploc/phploc" : "^3.0.1",
		"phpmd/phpmd" : "^2.4.3",
		"phpunit/phpunit" : "^5.5.3",
		"satooshi/php-coveralls" : "^1.0.1",
		"sebastian/phpcpd" : "^2.0.4",
		"squizlabs/php_codesniffer" : "^2.6.2",
		"symfony/yaml" : "^3.1.3"
	}
}